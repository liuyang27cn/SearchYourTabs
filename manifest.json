{
  "manifest_version": 2,

  "name": "Search through your tabs",
  "description": "This extension searches content of all the tabs",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "search.html",
    "default_title": "Search"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ]
}
